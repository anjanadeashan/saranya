# Environment Variables Template
# Copy this file to .env and update values as needed

# Backend API Configuration
REACT_APP_API_URL=https://api.saranyainternational.online/api
REACT_APP_API_TIMEOUT=10000

# Application Configuration
REACT_APP_NAME=Saranya International
REACT_APP_VERSION=1.0.0

# Authentication Configuration
REACT_APP_TOKEN_STORAGE_KEY=token
REACT_APP_SESSION_TIMEOUT=24

# Feature Flags
REACT_APP_ENABLE_DEBUG_LOGGING=false
REACT_APP_ENABLE_ANALYTICS=false

# UI Configuration
REACT_APP_TOAST_AUTO_CLOSE=3000
REACT_APP_PAGINATION_SIZE=10